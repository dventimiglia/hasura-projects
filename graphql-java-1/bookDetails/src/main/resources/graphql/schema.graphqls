# -*- mode: graphql; -*-

type Query {
  bookById(id: ID): Book
  books: [Book]!
  accounts: [account]!
  account_by_pk(id: ID): account
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

type account {
  id: ID
  name: String
  created_at: String
  updated_at: String
}
